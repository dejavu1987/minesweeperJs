{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint-config-airbnb-base"
    ],
    "include":"./src/",
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
      "comma-dangle": "off"
    }
}
